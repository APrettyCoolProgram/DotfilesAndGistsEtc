{
    "AttributeIndentation": 0,
    "AttributeIndentationStyle": 1,
    "AttributesTolerance": 0,
    "KeepFirstAttributeOnSameLine": false,
    "MaxAttributeCharactersPerLine": 0,
    "MaxAttributesPerLine": 0,
    "NewlineExemptionElements": "RadialGradientBrush, GradientStop, LinearGradientBrush, ScaleTransfom, SkewTransform, RotateTransform, TranslateTransform, Trigger, Condition, Setter",
    "RemoveDesignTimeReferences":  false,
    "SeparateByGroups": true,
    "AttributeOrderingRuleGroups": [
		"x:Class",
		"xmlns",
		"xmlns:x",
		"xmlns:*",
		"x:Key, Key",
		"x:Uid, Uid",
		"mc:Ignorable, d:IsDataSource, d:LayoutOverrides, d:IsStaticText",
		"x:Name, Name",
		"Grid.Column, Grid.Row, Grid.ColumnSpan, Grid.RowSpan",
		"Canvas.Left, Canvas.Right, Canvas.Bottom, Canvas.Top",
		"Height, Width, MinHeight, MinWidth, MaxHeight, MaxWidth",
		"HorizontalAlignment, VerticalAlignment",
		"Margin, Padding",
		"SnapsToDevicePixels",
		"TextWrapping",
		"Background, Foreground",
		"Content, Header, Text, Title",
		"HorizontalContentAlignment, VerticalContentAlignment",
		"Panel.ZIndex",
		"BorderBrush, BorderThickness",
		"TabStripPlacement",
		"Click"
    ],
    "EnableAttributeReordering": true,
    "FirstLineAttributes": "",
    "OrderAttributesByName": true,
    "PutEndingBracketOnNewLine": false,
    "RemoveEndingTagOfEmptyElement": true,
    "RootElementLineBreakRule": 0,
    "SpaceBeforeClosingSlash": true,
    "ReorderCanvasChildren": false,
    "ReorderGridChildren": false,
    "ReorderSetters": 0,   
    "ReorderVSM": 2,
    "FormatMarkupExtension": true,
    "NoNewLineMarkupExtensions": "x:Bind, Binding",
    "CommentPadding": 2,   
    "FormatOnSave": true,
    "ThicknessAttributes": "Margin, Padding, BorderThickness, ThumbnailClipMargin",
    "ThicknessSeparator": 2,
}